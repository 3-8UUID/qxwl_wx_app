.page{
    background-color: #ffffff;
    height: 100%;
    width: 100%;
}
.top{
    height: 25%;
    width: 100%;
    padding: 30px 0px;
}
.center{
    padding: 25px 0px;
    height: 25%;
    width: 98%;
    border: solid 5px rgba(229, 224, 224, 0.99);
    background-color: #fcf8f8;
    border-radius: 15px;
}

/*////////////////////////////////////////////////////*/
.page-index{
    background-color: #efede9;
    height: 100%;
    width: 100%;
}
.page-index .changeTitleImage{
    position: absolute;
    top: 195px;
    right: 5px;
}
/* 头部 */
.header-container {
    display: flex;
    flex-direction: row;
    height: 450rpx;
    background-color: #fff;
    background-repeat: no-repeat;
    background-position: center;
    background-size:100% 100%;
    -moz-background-size:100% 100%;
}

.header-bg{
    width: 100%;
    height: 100%;
    z-index: 0.1;
}

.bottom_btn {
    z-index: 2;
    background-color: #efede9;
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
}

.bottom-bag{
    z-index: 1;
    background-color: #efede9;
    position: relative;
    bottom: 0;
    width: 100%;
    height: 300px;
}

.body-context{
    width: 100%;
    height: 150px;
    background-color: #FFF;
    border-radius: 15px;
    border: #cdc5c5 solid 2px;
}

.add-icon{
    margin: 5px 0px;
}